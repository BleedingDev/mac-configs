# Space focus
ctrl - 0x12 : yabai -m space --focus 1
ctrl - 0x13 : yabai -m space --focus 2
ctrl - 0x14 : yabai -m space --focus 3
ctrl - 0x15 : yabai -m space --focus 4
ctrl - 0x17 : yabai -m space --focus 5
ctrl - 0x16 : yabai -m space --focus 6
ctrl - 0x1A : yabai -m space --focus 7

# Move to space
shift + alt - 0x12 : yabai -m window --space 1; yabai -m space --focus 1
shift + alt - 0x13 : yabai -m window --space 2; yabai -m space --focus 2
shift + alt - 0x14 : yabai -m window --space 3; yabai -m space --focus 3
shift + alt - 0x15 : yabai -m window --space 4; yabai -m space --focus 4
shift + alt - 0x17 : yabai -m window --space 5; yabai -m space --focus 5
shift + alt - 0x16 : yabai -m window --space 6; yabai -m space --focus 6
shift + alt - 0x1A : yabai -m window --space 7; yabai -m space --focus 7

# Display focus
alt - 0x12 : yabai -m display --focus 1
alt - 0x13 : yabai -m display --focus 2
alt - 0x14 : yabai -m display --focus 3

# focus window
alt - h : yabai -m window --focus west
alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - l : yabai -m window --focus east

# swap window
shift + alt - h : yabai -m window --swap west
shift + alt - j : yabai -m window --swap south
shift + alt - k : yabai -m window --swap north
shift + alt - l : yabai -m window --swap east

# move window
shift + cmd - h : yabai -m window --warp west
shift + cmd - j : yabai -m window --warp south
shift + cmd - k : yabai -m window --warp north
shift + cmd - l : yabai -m window --warp east

# Rest magic
shift + alt - p : yabai -m window --toggle pip
shift + alt - f : yabai -m window --toggle float

# VPN magic
cmd + alt - l : networksetup -connectpppoeservice "Prague"
cmd + alt - k : networksetup -disconnectpppoeservice "Prague" 
